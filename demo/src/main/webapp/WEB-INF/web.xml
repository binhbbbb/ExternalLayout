<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID" version="2.5">
	<display-name>ExternalLayout</display-name>
	<context-param>
		<description>
  	Vaadin production mode</description>
		<param-name>productionMode</param-name>
		<param-value>false</param-value>
	</context-param>
	<servlet>
		<servlet-name>ExternalLayout Demo</servlet-name>
		<servlet-class>com.vaadin.server.VaadinServlet</servlet-class>
		<init-param>
			<description>
  		Vaadin UI to start</description>
			<param-name>UI</param-name>
			<param-value>org.vaadin.risto.externallayout.ExternalLayoutDemoUI</param-value>
		</init-param>
		<init-param>
			<description>
  		Application widgetset</description>
			<param-name>widgetset</param-name>
			<param-value>org.vaadin.risto.externallayout.widgetset.ExternalLayoutDemoWidgetset</param-value>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>ExternalLayout Demo</servlet-name>
		<url-pattern>/ExternalLayout/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ExternalLayout Demo</servlet-name>
		<url-pattern>/VAADIN/*</url-pattern>
	</servlet-mapping>
</web-app>